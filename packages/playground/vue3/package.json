{
  "name": "uni-preset-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "uni",
    "dev:mp": "uni -p mp-weixin",
    "dev:plugin": "cross-env UNI_OUTPUT_DIR=dist/dev/mp-weixin/plugin pnpm dev:mp --plugin",
    "dev:mp-plugin": "cross-env NODE_ENV=development UNI_PLATFORM=mp-weixin UNI_OUTPUT_DIR=dist/dev/mp-weixin/mp pnpm dev:mp --minify",
    "build:h5": "uni build",
    "build:mp": "uni build -p mp-weixin",
    "build:plugin": "cross-env UNI_OUTPUT_DIR=dist/build/mp-weixin/plugin pnpm run build:mp --plugin plugin",
    "test": "vite --debug"
  },
  "dependencies": {
    "@dcloudio/uni-app": "link:../../uni-app",
    "@dcloudio/uni-components": "link:../../uni-components",
    "@dcloudio/uni-h5": "link:../../uni-h5",
    "@dcloudio/uni-mp-weixin": "link:../../uni-mp-weixin",
    "vue": "3.2.45"
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@dcloudio/types": "^3.3.2",
    "@dcloudio/uni-cli-shared": "link:../../uni-cli-shared",
    "@dcloudio/vite-plugin-uni": "link:../../vite-plugin-uni",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^7.0.3",
    "es-module-lexer": "^0.9.3",
    "magic-string": "^0.27.0",
    "typescript": "^4.9.4",
    "vite": "4.0.4",
    "vite-plugin-inspect": "^0.7.15",
    "vue-tsc": "^1.0.24"
  }
}