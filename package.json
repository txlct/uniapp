{
  "private": true,
  "version": "3.0.0-3070320230222002-v3.0.10",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "all": "npm run lint && npm run format && npm run build && npm run test",
    "clean": "rm -rf node_modules **/*/node_modules && pnpm install",
    "build": "node scripts/build.js",
    "build:h5": "node scripts/build.js uni-app uni-cli-shared uni-h5 uni-h5-vite vite-plugin-uni uni-shared",
    "dev:h5": "node scripts/build.js -d uni-app uni-cli-shared uni-h5  uni-h5-vite vite-plugin-uni",
    "build:app": "node scripts/build.js uni-app-plus uni-app-vite uni-app-vue",
    "build:mp": "node scripts/build.js uni-cli-shared vite-plugin-uni uni-mp-vite uni-mp-compiler uni-mp-weixin",
    "dev:mp": "node scripts/build.js uni-cli-shared vite-plugin-uni uni-mp-vite uni-mp-compiler uni-mp-weixin -d",
    "dev:mp:p": "node scripts/build.js uni-cli-shared & node scripts/build.js vite-plugin-uni & node scripts/build.js uni-mp-vue & node scripts/build.js uni-mp-vite & node scripts/build.js uni-mp-compiler & node scripts/build.js uni-mp-weixin",
    "size": "npm run build size-check",
    "lint": "eslint packages/*/src/**/*.ts",
    "format": "prettier --write --parser typescript \"packages/**/*.ts?(x)\"",
    "test": "tsc -p ./test-dts/tsconfig.json && node scripts/test.js && jest",
    "test-dts": "tsc -p ./test-dts/tsconfig.json && tsc -p ./test-dts/tsconfig.build.json",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "e2e:ssr:dev": "cd packages/playground/ssr && npm run dev:ssr",
    "e2e:ssr:open": "cd packages/playground/ssr && npx cypress open",
    "release": "node scripts/release.js --skipTests",
    "release:mp": "node scripts/release.js uni-cli-shared vite-plugin-uni uni-mp-vue uni-mp-vite uni-mp-compiler uni-mp-weixin --skipTests --skipBuild",
    "release:h5": "node scripts/release.js uni-app uni-cli-shared uni-h5 uni-h5-vite vite-plugin-uni --skipTests --skipBuild --dry",
    "release:version": "pnpm release --skipTests --dry --tag",
    "release:dev": "pnpm release --local --skipTests --dry"
  },
  "types": "test-dts/index.d.ts",
  "tsd": {
    "directory": "test-dts"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false && pnpm run test-dts",
    "commit-msg": "node scripts/verifyCommit.js $1"
  },
  "lint-staged": {
    "packages/**/*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@dcloudio/types": "3.2.11",
    "@dcloudio/uni-api": "workspace:*",
    "@dcloudio/uni-app": "https://gitpkg.now.sh/txlct/uniapp/packages/uni-app?v3.0.10",
    "@jest/types": "^29.5.0",
    "@microsoft/api-extractor": "^7.34.4",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-strip": "^3.0.2",
    "@rollup/plugin-terser": "^0.2.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/compiler-sfc": "3.2.47",
    "@vue/reactivity": "3.2.47",
    "@vue/runtime-core": "3.2.47",
    "@vue/runtime-dom": "3.2.47",
    "@vue/shared": "3.2.47",
    "core-js": "^2.6.12",
    "enquirer": "^2.3.6",
    "eslint": "^8.36.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.1.0",
    "jest": "^29.5.0",
    "lint-staged": "^10.5.4",
    "mini-types": "^0.1.7",
    "minimist": "^1.2.8",
    "miniprogram-api-typings": "^3.9.0",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.4",
    "rollup": "^3.19.1",
    "rollup-plugin-jscc": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "semver": "^7.3.8",
    "simple-git-hooks": "^2.8.1",
    "terser": "^5.16.6",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "vite": "4.1.1",
    "vue": "3.2.47",
    "vue-router": "^4.1.6",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "@babel/plugin-transform-block-scoping": "7.19.4"
  },
  "packageManager": "pnpm@7.5.2",
  "name": "uni-app-next"
}
